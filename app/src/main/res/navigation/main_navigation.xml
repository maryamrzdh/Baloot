<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_navigation"
    app:startDestination="@id/placeholderFragment">
    <fragment
        android:id="@+id/placeholderFragment"
        android:name="com.example.baloot_maryammemarzadeh.ui.main.PlaceholderFragment"
        android:label="PlaceholderFragment" >
        <argument
            android:name="article"
            app:argType="com.example.baloot_maryammemarzadeh.model.Article"
            />
        <action
            android:id="@+id/action_placeholderFragment_to_detailsFragment2"
            app:destination="@id/detailsFragment2" />
    </fragment>
    <fragment
        android:id="@+id/placeholderFragment2"
        android:name="com.example.baloot_maryammemarzadeh.ui.second.PlaceholderFragment2"
        android:label="PlaceholderFragment2" />
    <fragment
        android:id="@+id/detailsFragment2"
        android:name="com.example.baloot_maryammemarzadeh.ui.main.DetailsFragment"
        android:label="DetailsFragment" />
    <activity
        android:id="@+id/splashActivity3"
        android:name="com.example.baloot_maryammemarzadeh.ui.splash.SplashActivity"
        android:label="activity_splash"
        tools:layout="@layout/activity_splash" />
    <activity
        android:id="@+id/mainActivity4"
        android:name="com.example.baloot_maryammemarzadeh.ui.MainActivity"
        android:label="MainActivity" />
</navigation>